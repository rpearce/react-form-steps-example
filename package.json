{
  "name": "react-form-steps-example",
  "version": "1.0.0",
  "description": "Form steps example with React.js",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf before/build/ && mkdir before/build/ && rm -rf after/build/ && mkdir after/build/",
    "build:before": "browserify before/src/browser.js -o before/build/app.js",
    "build:after": "browserify after/src/browser.js -o after/build/app.js",
    "build": "npm run clean && npm run build:before && npm run build:after",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpearce/react-form-steps-example.git"
  },
  "keywords": [
    "react",
    "form",
    "steps",
    "form steps"
  ],
  "author": "Robert Pearce <me@robertwpearce.com> (http://robertwpearce.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rpearce/react-form-steps-example/issues"
  },
  "homepage": "https://github.com/rpearce/react-form-steps-example#readme",
  "devDependencies": {
    "babel": "^5.8.29",
    "babelify": "^6.4.0",
    "browserify": "^12.0.1",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "history": "^1.13.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-router": "^0.13.5"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  }
}
